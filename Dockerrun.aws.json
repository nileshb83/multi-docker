{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name" : "client",
            "image": "nileshb83/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name" : "server",
            "image": "nileshb83/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name" : "worker",
            "image": "nileshb83/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name" : "nginx",
            "image": "nileshb83/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "partMappings": [
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],   
            "links":["client","server"]
        }
    ]
}